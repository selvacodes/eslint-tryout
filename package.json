{
  "name": "eslint-tryout",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index",
    "lint": "eslint index.js",
    "standard-fix": "standard --fix index.js",
    "prettier-write": "prettier --write --no-semi \"./index.js\"",
    "fix-lint": "eslint --fix index.js",
    "format": "yarn run standard-fix && yarn run prettier-write",
    "fix": "yarn run format && yarn run fix-lint"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^2.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "prettier": "^1.3.1",
    "standard": "^10.0.2"
  }
}
